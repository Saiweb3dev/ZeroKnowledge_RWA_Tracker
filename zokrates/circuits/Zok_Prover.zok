
def main(
    field[20] reqAddress, 
    field reqId, 
    field reqData, 
    field[20] OrgAddress, 
    field OrgId, 
    field OrgData
) -> bool {
    bool mut isAddressEqual = true;
    for u32 i in 0..20 {
        isAddressEqual = isAddressEqual && (reqAddress[i] == OrgAddress[i]);
    }

    bool isIdEqual = reqId == OrgId;
    bool isDataEqual = reqData == OrgData;

    return isAddressEqual && isIdEqual && isDataEqual;
}
